<component name="vncEnumPropEditor" extends="ListPanel" initialFocus="labelList">
	<script type="text/brightscript" uri="vncEnumPropEditor.brs" />

	<interface>
		<field id="title" type="string" />
		<field id="key" type="string" />
		<field id="value" type="string" onChange="onSetValue" />
	</interface>

	<children>
		<LabelList id="labelList">
			<ContentNode role="content">
				<ContentNode />
			</ContentNode>
		</LabelList>
	</children>
</component>
